                        <script>
                     // Obter a localização do usuário no navegador
if ("geolocation" in navigator) {
  navigator.geolocation.getCurrentPosition(function(position) {
    const latitude = position.coords.latitude;
    const longitude = position.coords.longitude;

    // Enviar as coordenadas para o servidor Node.js usando uma solicitação HTTP (por exemplo, AJAX)
    const data = { latitude, longitude };

    fetch('/risc', {
      method: 'POST',
      headers: {
        'Content-Type': 'application/json',
      },
      body: JSON.stringify(data),
    })
    .then(response => {
      if (response.ok) {
        console.log('Localização enviada com sucesso ao servidor.');
      } else {
        console.error('Erro ao enviar localização ao servidor.');
      }
    })
    .catch(error => {
      console.error('Erro ao enviar localização ao servidor:', error);
    });
  }, function(error) {
    console.error('Erro ao obter a localização do usuário:', error);
  });
} else {
  console.error('O navegador não suporta a API de Geolocalização.');
}


                        </script>
					


					

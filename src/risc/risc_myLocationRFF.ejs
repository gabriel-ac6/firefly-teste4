							<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
							<script>
								// Valor dinâmico obtido da variável RFF
								
								const RFF = <%= RFF %>;
								
								// Função para animar o número com o valor dinâmico e definir o nível de risco
								function animateNumberWithDynamicValue(dynamicValue) {
								  const animatedNumberElement = document.getElementById('animated-number');
								  const riskLevelElement = document.getElementById('risk-level');
								  let currentNumber = 0;
								  const animationDuration = 3000; // Duração da animação em milissegundos
								  const interval = 50; // Intervalo de atualização em milissegundos
							  
								  const increment = (dynamicValue - currentNumber) / (animationDuration / interval);
							  
								  const intervalId = setInterval(() => {
									currentNumber += increment;
									animatedNumberElement.textContent = Math.round(currentNumber);
							  
									if (currentNumber >= dynamicValue) {
									  clearInterval(intervalId);
									  animatedNumberElement.textContent = dynamicValue; // Garante que o número final seja exibido corretamente
							  
									  // Defina o nível de risco com base no valor dinâmico
									  if (dynamicValue < 0.15) {
										riskLevelElement.textContent = 'Mínimo';
										riskLevelElement.className = 'level-of-risk minimo';
									  } else if (dynamicValue <= 0.40) {
										riskLevelElement.textContent = 'Baixo';
										riskLevelElement.className = 'level-of-risk baixo';
									  } else if (dynamicValue <= 0.70) {
										riskLevelElement.textContent = 'Médio';
										riskLevelElement.className = 'level-of-risk medio';
									  } else if (dynamicValue <= 0.95) {
										riskLevelElement.textContent = 'Alto';
										riskLevelElement.className = 'level-of-risk alto';
									  } else {
										riskLevelElement.textContent = 'Crítico';
										riskLevelElement.className = 'level-of-risk critico';
									  }
									}
								  }, interval);
								}
							  
								// Inicie a animação com o valor dinâmico (RFF)
								window.addEventListener('load', function() {
								  animateNumberWithDynamicValue(RFF);
								});
							  </script>
							  

